services:
  webapi:
    build:
      context: C:\Users\mohapap\source\repos\coreMicroservices_WithTest
      dockerfile: WebApi/Dockerfile
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44371'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: webapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.1/WebApi.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 51480
      target: 80
    - published: 44371
      target: 443
    volumes:
    - C:\Users\mohapap\source\repos\coreMicroservices_WithTest\WebApi:/app:rw
    - C:\Users\mohapap\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Users\mohapap\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\mohapap\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\mohapap\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'